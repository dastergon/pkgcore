INTERNAL_FUNCS+=( get_libdir )

get_libdir() {
	local CONF_LIBDIR="lib"
	local LIBDIR_VAR="LIBDIR_${ABI}"
	[[ -n ${ABI} && -n ${!LIBDIR_VAR} ]] && CONF_LIBDIR=${!LIBDIR_VAR}
	echo "${CONF_LIBDIR}"
}

